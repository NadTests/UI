local players = game:GetService("Players"):GetPlayers()
local currentPlayer = 1
local loopActive = true
local spectating = false

game:GetService("UserInputService").InputEnded:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.R then
        loopActive = false
        if spectating then
            spectating = false
            game:GetService("Workspace").CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character
        end
    end
end)

while loopActive do
    local player = players[currentPlayer]
    game:GetService("Workspace").CurrentCamera.CameraSubject = player.Character
    currentPlayer = currentPlayer + 1
    if currentPlayer > #players then
        currentPlayer = 1
    end
    if not spectating then
        spectating = true
    end
    wait(1)
end

end)
