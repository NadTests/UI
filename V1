    if v.ClassName == "Tool" and v.Name == name then
        v.Parent = game.Players.LocalPlayer.Character
        v.GripPos = Vector3.new(x, y, z)
        x = x + increment
        y = y + increment1
        z = z + increment2
        task.wait()
    end
end
